        android:rotation="+90"
        android:scaleX=".1"
        android:scaleY=".1"
        android:src="@drawable/arrowu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rechargebutton"
